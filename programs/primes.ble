fn prime(n){
    p = true;
    if n < 2 or n % 2 == 0 then p = false;
    if n == 2 then p = true;
    for j = 3 to n step 2 do{
        if n % j == 0 then{
            p = false;
        };
    };
    p;
};

if module == "<main>" then {
    disp "From: ":;
    from = input;
    disp "To: ":;
    limit = input + 1;

    for i = from to limit step 2 do{
        if prime(i) then{
            disp i;
        };
    };
};