fn prime(n){
    p = true;
    for j = 3 to n step 2 do{
        if i % j == 0 then{
            p = false;
        };
    };
    p;
};

disp "From: ":;
from = input;
disp "To: ":;
limit = input + 1;

if limit > 2 and from <= 2 then{
    disp 2;
};
if from < 3 then{
    from = 3;
}elif from % 2 == 0 then{
    from = from + 1;
};
for i = from to limit step 2 do{
    if prime(i) then{
        disp i;
    };
};
