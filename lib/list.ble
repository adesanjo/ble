fn range(start, end){
    l = []
    for i = start to end{
        l = l + [i]
    }
    l
}

fn map(f, l){
    for i = 0 to +l{
        l(i) = f(l(i))
    }
    l
}

fn filter(f, l){
    nl = []
    for each ll in l{
        if f(ll) nl = nl + [ll]
    }
    nl
}

fn builtin reduce(f, l){
    if +l > 0{
        res = l(0)
        for i = 1 to +l{
            res = f(res, l(i))
        }
        res
    }else{
        none
    }
}

moduleName = "list"