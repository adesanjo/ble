fn builtin min(a, b){
    if a < b a
    else b
}

fn builtin max(a, b){
    if a > b a
    else b
}

fn builtin minl(l){
    m = l(0)
    for each i in l{
        if i < m m = i;
    }
    m
}

fn builtin maxl(l){
    m = l(0)
    for each i in l{
        if i > m m = i;
    }
    m
}

fn builtin abs(x){
    if x < 0 then -x
    else x
}

